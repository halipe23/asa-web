;
; BIND data file for local loopback interface
;
$TTL	604800
@	IN	SOA	ns.asa.br. root.asa.br. (
			      1		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	ns.
@	IN	A	127.0.0.1
@	IN	AAAA	::1
www IN 	A 		10.24.25.138
portal IN CNAME www
