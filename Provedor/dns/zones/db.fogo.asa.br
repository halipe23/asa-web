;
; BIND data file for local loopback interface
;
$ORIGIN fogo.asa.br.
$TTL	604800
@	IN	SOA	ns.fogo.asa.br. root.fogo.asa.br. (
			      1		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@		IN	NS	ns.fogo.asa.br.
@		IN	MX	10	mail
@		IN	A	192.168.15.4

ns      IN  A   192.168.15.4
mail    IN  A   192.168.15.4
www     IN  A   192.168.15.4
proxy   IN  CNAME www